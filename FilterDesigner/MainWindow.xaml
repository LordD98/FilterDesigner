<Window x:Class="FilterDesigner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FilterDesigner"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition Width="100"/>
		</Grid.ColumnDefinitions>
		<Canvas x:Name="canvas" 
				Margin="5,5,5,4.667" 
				SnapsToDevicePixels="True" 
				ClipToBounds="True" 
				Background="#FFFAFAFA"/>
		<Grid Grid.Column="1" Margin="0,0,5,0">
			<Grid.RowDefinitions>
				<RowDefinition/>
				<RowDefinition/>
				<RowDefinition Height="30"/>
				<RowDefinition Height="30"/>
				<RowDefinition Height="30"/>
			</Grid.RowDefinitions>
			<ComboBox x:Name="cbxNet1" Margin="0,5" Grid.Row="2"/>
			<ComboBox x:Name="cbxNet2" Margin="0,5" Grid.Row="3"/>
			<Button x:Name="btnTest" Content="Get Impedance" Margin="0,5" Click="BtnTest_Click" Grid.Row="4"/>
		</Grid>
	</Grid>
</Window>
