<Window x:Class="FilterDesigner.OutputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FilterDesigner"
		xmlns:LaTeX="clr-namespace:WpfMath.Controls;assembly=WpfMath"
        mc:Ignorable="d"
        Title="Result" SizeToContent="WidthAndHeight" MinWidth="400" MinHeight="250" Icon="Symbol.png" KeyDown="OutputWindow_KeyDown" SizeChanged="OutputWindow_SizeChanged">
    <Grid>
		<LaTeX:FormulaControl x:Name="OutputExpression" Formula="\infty" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="20"/>
		<Canvas x:Name="CvsGraph" Margin="10" Background="#FFD7D7FF" ClipToBounds="True"/>
	</Grid>
</Window>
