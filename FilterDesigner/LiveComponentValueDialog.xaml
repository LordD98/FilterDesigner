<Window x:Class="FilterDesigner.LiveComponentValueDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FilterDesigner"
        mc:Ignorable="d"
        Title="Change Component Values" Height="450" Width="300" Icon="Symbol.png">
	<Border BorderBrush="#FF484848" BorderThickness="1" Margin="1">
		<ScrollViewer>
			<Grid x:Name="GridComponents" ScrollViewer.CanContentScroll="True" Margin="1" SnapsToDevicePixels="True" ClipToBounds="True">
				<Grid.RowDefinitions>
				</Grid.RowDefinitions>
			</Grid>
		</ScrollViewer>
	</Border>
</Window>
